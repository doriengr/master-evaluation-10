<dl class="definition-list">
    {% for item in items %}
        <dt>
            {% if item.icon %}
                <span class="sr-only">{{ item.label }}</span>
                {% include item.icon %}
            {% else %}
                {{ item.label }}
            {% endif %}
        </dt>
        <dd>
            {{ item.value }}
        </dd>
    {% endfor %}
</dl>
